{
  "name": "dialektli",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "yarn generate:sitemap && prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "check:types": "tsc --pretty --noEmit",
    "check:format": "prettier --check .",
    "postinstall": "yarn prisma generate",
    "generate:types": "graphql-codegen",
    "generate:types:watch": "graphql-codegen --watch",
    "format": "prettier --write .",
    "prisma:migrate": "prisma migrate dev && yarn print:db",
    "prisma:generate": "prisma generate",
    "prisma:studio": "prisma studio",
    "prepare": "husky",
    "generate:sitemap": "tsx scripts/generate-sitemap.ts",
    "seed": "prisma db seed",
    "print:db": "cp prisma/schema.prisma ./schema.prisma && sed -i.bak -e 's| // uses connection pooling||g' -e '/POSTGRES_URL_NON_POOLING/d' ./schema.prisma && yarn prisma-uml ./schema.prisma --output svg --file ./db.svg && rm ./schema.prisma.bak && rm ./schema.prisma"
  },
  "prisma": {
    "seed": "ts-node --transpile-only ./prisma/seed/index.ts"
  },
  "dependencies": {
    "@apollo/client": "^3.9.5",
    "@apollo/server": "^4.10.1",
    "@as-integrations/next": "^3.0.0",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.12",
    "@mui/joy": "^5.0.0-beta.30",
    "@mui/material": "^5.15.12",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@pothos/core": "^3.41.0",
    "@pothos/plugin-prisma": "^3.65.0",
    "@pothos/plugin-simple-objects": "^3.7.0",
    "@prisma/client": "^5.10.2",
    "@tanstack/match-sorter-utils": "^8.11.8",
    "@tanstack/react-table": "^8.13.2",
    "accept-language-parser": "^1.5.0",
    "bcrypt": "^5.1.1",
    "cookies": "^0.9.1",
    "crypto": "^1.0.1",
    "dayjs": "^1.11.10",
    "graphql": "^16.8.1",
    "graphql-middleware": "^6.1.35",
    "graphql-scalars": "^1.22.5",
    "graphql-shield": "^7.6.5",
    "graphql-tools": "^9.0.1",
    "i18next": "^23.10.0",
    "lodash": "^4.17.21",
    "next": "14.1.2",
    "next-auth": "^4.24.6",
    "next-i18next": "^15.2.0",
    "next-recaptcha-v3": "^1.3.0",
    "next-seo": "^6.5.0",
    "next-with-apollo": "^5.3.0",
    "nodemailer": "^6.9.11",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.0.5",
    "react-images-uploading": "^3.1.7",
    "react-share-lite": "^1.1.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/client-preset": "^4.2.4",
    "@graphql-codegen/schema-ast": "4.0.2",
    "@parcel/watcher": "^2.4.1",
    "@types/accept-language-parser": "^1.5.6",
    "@types/bcrypt": "^5.0.2",
    "@types/cookies": "^0.9.0",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.24",
    "@types/nodemailer": "^6.4.14",
    "@types/react": "^18.2.63",
    "@types/react-dom": "^18.2.20",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.1.2",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.0",
    "globby": "^14.0.1",
    "husky": "^9.0.11",
    "prettier": "^3.2.5",
    "prisma": "^5.10.2",
    "prisma-uml": "^1.1.18",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  },
  "packageManager": "yarn@4.1.0"
}
